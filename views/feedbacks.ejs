<html>
  <head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <link
      rel="stylesheet"
      href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css"
    />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
    <link rel="stylesheet" href="/css/style.css" />
    <title>Feedbacks</title>
  </head>

  <body>
    <div class="container">
      <h1 class="text-center" style="margin: 20">Feedbacks</h1>
      <table style="width: 90%" class="table table-striped table-bordered">
        <thead>
          <tr>
            <th scope="col" style="width: 300">Username</th>
            <th scope="col">Feedback</th>
          </tr>
        </thead>
        <tbody>
          <% for (var feed in data ) { %>
          <tr>
            <td><%= data[feed].username %></td>
            <td><%= data[feed].feedback %></td>
          </tr>
          <% } %>
        </tbody>
      </table>
    </div>
  </body>
</html>
